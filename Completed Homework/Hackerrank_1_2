#Exercise 1
def is_leap(year):
    leap = False
    
    if(year % 4 == 0):
        leap = True
        if(year % 100 == 0):
            leap = False
            if(year % 400 == 0):
                leap = True        
    return leap
year = int(input())


#Exercise 2
for i in range(int(input())):
    try:
        a,b=map(int,input().split())
        print(a//b)
    except Exception as error1:
        print("Error Code:",error1)