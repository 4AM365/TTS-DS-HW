import re
text_to_search = '''
abc

abc

abc

MRT
Mr. T
Mr T
Mister T

!@#$%^&*()~

wtcralig@live.com
928-208-3220

test
'''

pattern = re.compile(r'abc')
matches = pattern.finditer(text_to_search)


for match in matches:
    print(match)
print(text_to_search[1:4])
print(text_to_search[6:9])
# add r inside the print(r"") means the string is RAW so it will be handled normally. We want regex to interpret the string and not let Python screw with it.
